<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <Button
        android:id="@+id/buttonPrimary"
        android:layout_width="327dp"
        android:layout_height="56dp"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="237dp"
        android:layout_marginLeft="@dimen/marginLeft"
        android:theme="@style/Button.Large"
        android:drawableTint="@color/yellow"
        android:Radius="@dimen/radius"
        android:background="@drawable/button_larg"
        android:layout_centerVertical="true"
        android:text="@string/button_primary"
        />

    <Button
        android:id="@+id/buttonSecondary"
        android:layout_width="327dp"
        android:layout_height="56dp"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="29dp"
        android:layout_marginLeft="@dimen/marginLeft"
        android:Radius="@dimen/radius"
        android:background="@drawable/button_larg"
        android:theme="@style/Button.Large"
        android:layout_centerVertical="true"
        android:text="@string/button_secondary"
        android:layout_below="@+id/buttonPrimary"/>

<include>la</include>


</RelativeLayout>